{
    "name": "esign/laravel-dynamate-signature",
    "description": "Display the Dynamate-branded signature",
    "keywords": [
        "esign",
        "dynamate-signature"
    ],
    "homepage": "https://github.com/esign/laravel-dynamate-signature",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jordy Van der Haegen",
            "email": "jordy@esign.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Esign\\DynamateSignature\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esign\\DynamateSignature\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esign\\DynamateSignature\\DynamateSignatureServiceProvider"
            ],
            "aliases": {
                "DynamateSignature": "Esign\\DynamateSignature\\Facades\\DynamateSignatureFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
